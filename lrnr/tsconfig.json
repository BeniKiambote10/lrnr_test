{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "baseUrl": ".",
    "paths": {
      "cypress/*": ["./node_modules/cypress/types/*"]
    }
  },
  "include": ["cypress/**/*.ts"],
  "exclude": ["node_modules"]
}
